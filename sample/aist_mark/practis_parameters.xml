<?xml version="1.0" encoding="UTF-8" ?>
<practis>
  <exhaustive>
    <variables>
      <variable name="x" data_type="Float">
        <conditions>
          <condition type="include" range_type="range">
            <start>-1.2</start>
            <end>1.4</end>
<!--            <step>0.001</step> --> <!-- not work well. to many values -->
<!--            <step>0.01</step>  -->
            <step>0.05</step>
          </condition>
        </conditions>
      </variable>
      <variable name="y" data_type="Float">
        <conditions>
          <condition type="include" range_type="range">
            <start>-0.2</start>
            <end>1.1</end>
<!--            <step>0.001</step> --> <!-- not work well. to many values -->
<!--            <step>0.01</step> -->
            <step>0.05</step>
          </condition>
        </conditions>
      </variable>
    </variables>
  </exhaustive>
</practis>
